# Polytrage Environment Variables
# Copy to .env and fill in your values.

# Discord webhook for notifications (optional)
POLYTRAGE_DISCORD_WEBHOOK=

# Override log level (DEBUG, INFO, WARNING, ERROR)
# POLYTRAGE_LOG_LEVEL=INFO

# Override scan interval (seconds)
# POLYTRAGE_SCAN_INTERVAL=60

# Override max markets per scan
# POLYTRAGE_MAX_MARKETS=100
